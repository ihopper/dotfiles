{
    "git.enableSmartCommit": true,
    "explorer.openEditors.visible": 0,
    "workbench.colorTheme": "One Monokai",
    "gruvboxMaterial.darkWorkbench": "high-contrast",
    "editor.tabCompletion": "on",
    "window.restoreFullscreen": true,
